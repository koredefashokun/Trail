{
	"extends": [
		"plugin:react/recommended",
		"plugin:import/errors",
		"plugin:import/warnings",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"react",
		"prettier"
	],
	"globals": {
		"document": false
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true,
			"experimentalDecorators": true
		}
	},
	"env": {
		"jest": true,
		"node": true
	},
	"rules": {
		"no-unused-vars": 1,
		"no-duplicate-imports": 0,
		"import/no-duplicates": 2,
		"react/display-name": 0,
		"import/named": 0,
		"import/no-unresolved": 0,
		"react/prop-types": [
			1,
			{
				"skipUndeclared": true
			}
		],
		"prettier/prettier": [
			"error",
			{
				"useTabs": true,
				"tabWidth": 2,
				"printWidth": 120,
				"singleQuote": true,
				"jsxBracketSameLine": true,
				"trailingComma": "none"
			}
		]
	}
}